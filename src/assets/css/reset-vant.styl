.diy-dialog .van-dialog__footer .van-dialog__confirm{
  color: #2957A5;
}
.diy-dialog .van-dialog__footer .van-dialog__cancel{
  color: #AAAAAA;
}
.diy-dialog .van-dialog__header{
  padding: 65px 0;
  font-weight: 500;
}
.diy-dialog .van-dialog__content .van-dialog__message--has-title{
  padding: 0 20px 40px 20px;
  margin: -50px 0 0 0;
}
.enterprise-dialog, .enterprise-alert{
  width: 530px!important;
  overflow: visible!important;
}
.enterprise-dialog:before, .enterprise-alert:before{
  content: '';
  width: 130px;
  height: 130px;
  display: block;
  margin: -38px auto 0 auto;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.enterprise-dialog-zhuyi:before, .enterprise-alert-zhuyi:before{
  background-image: url('../images/enterprise/zhuyi.png');
}
.enterprise-dialog-yaoqing:before, .enterprise-alert-yaoqing:before{
  background-image: url('../images/enterprise/yaoqing.png');
}
.enterprise-dialog-shibai:before, .enterprise-alert-shibai:before{
  background-image: url('../images/enterprise/shibai.png');
}
.enterprise-dialog-chenggong:before, .enterprise-alert-chenggong:before{
  background-image: url('../images/enterprise/chenggong.png');
}
.enterprise-dialog-qiyeshibai:before, .enterprise-alert-qiyeshibai:before{
  background-image: url('../images/enterprise/qiyeshibai.png');
}
.enterprise-dialog .van-dialog__header, .enterprise-alert .van-dialog__header{
  width: 450px;
  margin: 0 auto;
  padding: 30px 0;
}
.enterprise-dialog .van-hairline--top, .enterprise-alert .van-hairline--top{
  height: 100px;
  border-bottom-right-radius: 33px;
  border-bottom-left-radius: 33px;
}
.enterprise-dialog .van-hairline--top:after{
  border-color: #D8D8D8;
}
.enterprise-alert .van-hairline--top:after{
  border-top: 4px solid #888888;
}
.enterprise-dialog .van-hairline--left:after{
  border-color: #D8D8D8;
}
.enterprise-dialog .van-hairline--top .van-button, .enterprise-alert .van-hairline--top .van-button{
  height: 100%;
}
.enterprise-dialog .van-hairline--top .van-dialog__cancel{
  color: #aaaaaa;
}
.enterprise-dialog .van-hairline--top .van-dialog__confirm, .enterprise-alert .van-hairline--top .van-dialog__confirm{
  color: #2957A5;
}